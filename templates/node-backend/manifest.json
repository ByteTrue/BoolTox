{
  "id": "com.booltox.node-template",
  "version": "0.0.1",
  "name": "Node Backend Template",
  "description": "示例：Web 前端 + Node.js 后端(JSON-RPC)",
  "protocol": "^2.0.0",
  "runtime": {
    "ui": {
      "type": "webview",
      "entry": "index.html"
    },
    "backend": {
      "type": "node",
      "entry": "backend/server.cjs"
    }
  },
  "permissions": ["backend.register", "backend.message"],
  "window": {
    "width": 960,
    "height": 640
  }
}
