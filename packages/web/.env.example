# Clerk 认证配置
# 从 https://dashboard.clerk.com/ 获取这些密钥
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_xxxxx
CLERK_SECRET_KEY=sk_test_xxxxx

# Clerk 重定向 URL（可选配置）
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/dashboard
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/dashboard

# Sentry 错误追踪
# 从 https://sentry.io/ 创建项目并获取 DSN
NEXT_PUBLIC_SENTRY_DSN=https://your_dsn_here@sentry.io/your_project_id
SENTRY_ORG=your_org_name
SENTRY_PROJECT=your_project_name
# 用于上传 source maps（可选）
SENTRY_AUTH_TOKEN=your_auth_token_here

# 额外的 Clerk 重定向 URL（可选）
NEXT_PUBLIC_CLERK_SIGN_IN_FALLBACK_REDIRECT_URL=/dashboard
NEXT_PUBLIC_CLERK_SIGN_UP_FALLBACK_REDIRECT_URL=/dashboard

# 开发环境插件目录（仅开发环境生效，可根据需要调整路径）
DEV_PLUGINS_DIR=/absolute/path/to/booltox-web/resources/plugins
