{
  "name": "@booltox/client",
  "private": true,
  "version": "0.0.1",
  "description": "BoolTox - Modular Electron toolbox with installable native modules",
  "author": "ByteTrue",
  "license": "CC-BY-NC-4.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:wsl": "./scripts/dev-wsl.sh",
    "dev:electron": "vite --host 0.0.0.0",
    "build": "tsc && vite build && electron-builder",
    "postbuild": "node ./scripts/generate-release-manifest.mjs",
    "tools": "node ./scripts/release-cli.mjs",
    "update-indexes": "node ./scripts/update-indexes.mjs",
    "prepare:uv": "node ./scripts/download-uv.mjs",
    "prepare:uv:win": "node ./scripts/download-uv.mjs win-x64",
    "prepare:uv:mac": "node ./scripts/download-uv.mjs darwin-x64 && node ./scripts/download-uv.mjs darwin-arm64",
    "prepare:uv:linux": "node ./scripts/download-uv.mjs linux-x64",
    "tool:pack": "node ./plugins/scripts/package-plugin.mjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:main": "vitest run --project main",
    "test:renderer": "vitest run --project renderer"
  },
  "dependencies": {
    "@booltox/shared": "workspace:*",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "adm-zip": "^0.5.16",
    "ajv": "^8.17.1",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "colord": "^2.9.3",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.23.16",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.2.1",
    "remark-gfm": "^4.0.1",
    "tar": "^7.5.2",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.24",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^4.0.15",
    "chokidar": "^3.6.0",
    "electron": "38.4.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.4.38",
    "semver": "^7.7.3",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^4.0.15"
  },
  "main": "dist-electron/main.js"
}
