{
  "id": "com.booltox.binary-sysmon-demo",
  "version": "1.0.0",
  "name": "系统监控（Binary TUI）",
  "description": "Go 编写的实时系统监控 TUI 工具，演示如何集成跨平台二进制工具",
  "protocol": "^2.0.0",
  "runtime": {
    "type": "cli",
    "backend": {
      "type": "process",
      "entry": {
        "darwin-arm64": "bin/sysmon-macos-arm64",
        "darwin-x64": "bin/sysmon-macos-x64",
        "win32-x64": "bin/sysmon-windows-x64.exe",
        "linux-x64": "bin/sysmon-linux-x64"
      }
    },
    "title": "系统监控 TUI"
  },
  "author": "BoolTox Team",
  "keywords": ["binary", "tui", "system", "monitor", "go", "bubbletea"]
}
